{
  "version": 2,
  "buildCommand": "npm run build --workspace=@cotion/shared && npm run build --workspace=@cotion/frontend",
  "outputDirectory": "packages/frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "devCommand": "npm run dev --workspace=@cotion/frontend",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
